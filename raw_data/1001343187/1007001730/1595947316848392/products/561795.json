: 1, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Icon"}, {"Key": "KAT_NEU_PRODBILD", "Value": "Icon"}]}, {"DocumentId": "5253bc700035d1c2abbaf1f4016d5c7e", "Url": "https://media.fischer.group/v7/_pim-media-prod_/Logos_Icons/NEUTRAL/W1_ZI_ETA_ETA-ALLG-NEUTRAL_N_SALL_AQQ_V4.jpg", "Filename": "W1_ZI_ETA_ETA-ALLG-NEUTRAL_N_SALL_AQQ_V4.jpg", "NameConvention": "ZI_Z_01_N", "OutputType": "PR", "Format": "jpg", "FileSize": 7581, "Type": "Icon", "Position": 1, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Icon"}, {"Key": "KAT_NEU_PRODBILD", "Value": "Icon"}]}], "Documents": {"AdditionalDocuments": [{"DocumentId": "fae7b019f5961a2e357ccc31397e0173", "Name": "Verkaufsunterlagen", "Description": "PowerFast II.", "Url": "https://fiproductmedia.azureedge.net/media/Marketing%20Materials/Sales%20Materials/VKU_POWERFAST-II_F_SDE_AIP_V1.pdf", "Language": "DE", "Filename": "VKU_POWERFAST-II_F_SDE_AIP_V1.pdf", "NameConvention": "VKU_F", "OutputType": "IP", "Format": "pdf", "FileSize": 3571822, "Type": "Sales Materials", "Position": 1, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Verkaufsunterlagen"}, {"Key": "INFO1", "Value": "PowerFast II."}, {"Key": "KAT_NEU_PRODBILD", "Value": "Verkaufsunterlagen"}], "CharacteristicValueNames": ["Verkaufsunterlagen"]}], "Approvals": [{"DocumentId": "80396344c8acd0fe7580c3e4644d2112", "Name": "ETA - Europ\u00e4ische Technische Bewertung", "Description": "Europ\u00e4ische Technische Bewertung f\u00fcr fischer Power-Fast II Schrauben - Schrauben zur Verwendung in Holzkonstruktionen", "Url": "https://fiproductmedia.azureedge.net/media/Certification%20Documents/ETA%20European%20Technical%20Assessment/fischer/ZD_ETAB_F_19-0175_SDE_AIP_V3.pdf", "Language": "DE", "Filename": "ZD_ETAB_F_19-0175_SDE_AIP_V3.pdf", "NameConvention": "ZD_ETAB_F", "OutputType": "IP", "ValidFrom": "2021-08-09T00:00:00", "Format": "pdf", "FileSize": 3302035, "Type": "ETA - European Technical Assessment", "Position": 1, "TextList": [{"Key": "INFO1", "Value": "Europ\u00e4ische Technische Bewertung f\u00fcr fischer Power-Fast II Schrauben \n- Schrauben zur Verwendung in Holzkonstruktionen"}], "AttributeList": [{"Name": "Zulassungsnummer", "Value": "ETA-19/0175", "Key": "ZULANR"}], "CharacteristicValueNames": ["ETA - Europ\u00e4ische Technische Bewertung"]}]}, "TextList": [{"Key": "BEZEICHNUNG", "Value": "FPF II WTP 6,0 x 80 BC 100"}, {"Key": "ARTIKELLANG", "Value": "FPF II WTP 6,0 x 80 BC 100"}, {"Key": "ECOMLANG", "Value": "fischer FPF II WTP 6,0 x 80 BC 100 Tellerkopf mit Innenstern TX Teilgewinde galvanisch verzinkt, blau passiviert"}, {"Key": "DOKBSTMARKCLAIM", "Value": "Die Spanplattenschraube mit Tellerkopf, Innenstern-Aufnahme und Teilgewinde."}, {"Key": "DOKBSTMARKDESCR", "Value": "Die fischer PowerFast II FPF II WT BC ist eine galvanisch verzinkte, blau passivierte Schraube mit Teilgewinde und Innenstern-Aufnahme. Der Tellerkopf mit Innenstern-TX-Antrieb garantiert einen gr\u00f6\u00dferen Zusammenzieheffekt sowie einen deutlich erh\u00f6hten Kopfdurchziehwiderstand. Die Ausf\u00fchrung mit Teilgewinde erm\u00f6glicht das feste Aneinanderziehen von Holzelementen."}, {"Key": "KAT_POS", "Value": "<div class=\"h9\">Die Spanplattenschraube mit Tellerkopf, Innenstern-Aufnahme und Teilgewinde."}, {"Key": "KAT_ANWENDUNG_GEN", "Value": "<div class=\"h10\">Anwendungen</div><ul><li>F\u00fcr die Verwendung in tragenden Holzkonstruktionen, zur Verbindung von Teilen aus Vollholz, Brettschichtholz, Holzwerkstoffplatten, etc.</li><li>F\u00fcr Verbindungen von Metallteilen auf Holz, wie z. B. Metallbeschl\u00e4gen, Winkeln, Balkenschuhen und sonstigen Metall- und Holzverbindungen.</li><li>F\u00fcr Anwendungen mit gepr\u00fcften Lasten im fischer D\u00fcbel (z. B. DuoPower und UX).</li></ul>"}, {"Key": "KAT_BAUSTOFF_GEN", "Value": "<div class=\"h10\">Baustoffe</div><ul><li>Vollholz (Nadel- und Laubholz)</li><li>Brettschichtholz</li><li>Brettsperrholz</li><li>Furnierschichtholz</li><li>\u00e4hnlich verleimte Holzbauteile und Holzwerkstoffplatten</li></ul>"}, {"Key": "POCTOPFEATURESGEN", "Value": "<div class=\"h10\">Top Features</div><ul><li>Die Schraubengeometrie der PowerFast II sorgt f\u00fcr eine schnelle, komfortable und flexible Montage.</li><li>Die Spanplattenschraube hat ein deutlich reduzierteres Spaltverhalten im Vergleich zu handels\u00fcblichen Schrauben.</li><li>Die galvanische Verzinkung, blau passiviert, enth\u00e4lt keine Chrom VI Anteile und ist damit sehr umweltvertr\u00e4glich.</li></ul>"}, {"Key": "KAT_VORTEILNUTZEN", "Value": "<div class=\"h10\">Vorteile</div><ul><li>Mit Tellerkopf f\u00fcr einen gr\u00f6\u00dferen Zusammenzieheffekt sowie einen deutlich erh\u00f6hten Kopfdurchziehwiderstand.</li><li>Die erh\u00f6hte Gewindesteigung der Schraube reduziert die Einschraubzeit deutlich.</li><li>Die Schraubenspitze mit den drei Rippen sorgt f\u00fcr ein schnelles Anbissverhalten und Vorfr\u00e4sen.</li><li>Die Schaftfr\u00e4srippen in Kombination mit der Kernfr\u00e4sergeometrie reduzieren den Einschraubwiderstand sp\u00fcrbar.</li><li>Die PowerFast II ist eine Schraube mit gepr\u00fcften Lasten im fischer D\u00fcbel (z. B. DuoPower oder UX).</li></ul>"}, {"Key": "KAT_FUNKTION", "Value": "<div class=\"h10\">Funktionsweise / Montage</div><ul><li>Schrauben mit Teilgewinde k\u00f6nnen Holzbauteile fest gegeneinander verspannen.</li><li>Schrauben mit Senkkopf k\u00f6nnen oberfl\u00e4chenb\u00fcndig im Holz versenkt werden.</li></ul>"}, {"Key": "DOKBSTTOPFEATURE", "Value": "Die Schraubengeometrie der PowerFast II sorgt f\u00fcr eine schnelle, komfortable und flexible Montage."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Die Spanplattenschraube hat ein deutlich reduzierteres Spaltverhalten im Vergleich zu handels\u00fcblichen Schrauben."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Die galvanische Verzinkung, blau passiviert, enth\u00e4lt keine Chrom VI Anteile und ist damit sehr umweltvertr\u00e4glich."}], "Attributes": [{"AttributeKey": "ZULASSUNG_E", "AttributeName": "ETA-Zulassung", "Value": "relevant"}, {"AttributeKey": "DURCHM", "AttributeName": "Durchmesser", "Unit": "[mm]", "Value": "6,0", "TechnicalName": "<div class=\"h6\">d</div>"}, {"AttributeKey": "LAENGE", "AttributeName": "L\u00e4nge", "Unit": "[mm]", "Value": "80", "TechnicalName": "<div class=\"h6\">L</div>"}, {"AttributeKey": "ANTRIEB", "AttributeName": "Antrieb", "Value": "TX30"}, {"AttributeKey": "GEW_LAE", "AttributeName": "Gewindel\u00e4nge", "Unit": "[mm]", "Value": "45", "TechnicalName": "<div class=\"h6\">l<sub>g</sub></div>"}, {"AttributeKey": "MENGE", "AttributeName": "Menge", "Unit": "[St\u00fcck]", "Value": "100"}, {"AttributeKey": "EAN_CODE", "AttributeName": "GTIN (EAN-Code)", "Value": "4048962437386"}], "FilterAttributes": [{"FilterId": "Pr\u00fcfzeichen / Zulassungen_A", "FilterName": "Pr\u00fcfzeichen / Zulassungen", "ValueList": ["ETA - Europ\u00e4isch Technische Bewertung"], "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "A", "ValueList": ["A"]}]}, {"FilterId": "L\u00e4nge_N", "FilterName": "L\u00e4nge", "Unit": "[mm]", "Value": "80", "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "N", "ValueList": ["N"]}]}, {"FilterId": "Profi / DIY_A", "FilterName": "Profi / DIY", "ValueList": ["Profi"], "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "A", "ValueList": ["A"]}]}, {"FilterId": "Durchmesser_N", "FilterName": "Durchmesser", "Unit": "[mm]", "Value": "6,0", "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "N", "ValueList": ["N"]}]}, {"FilterId": "Typ_A", "FilterName": "Typ", "ValueList": ["Spanplattenschraube"], "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "A", "ValueList": ["A"]}]}]}