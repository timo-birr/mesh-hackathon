, "Approvals": [{"DocumentId": "5776128fad38be5f2ed8d6793a23a747", "Name": "EPD - Europ\u00e4ische Umweltdeklaration", "Description": "Umwelt Produktdeklaration f\u00fcr fischer D\u00e4mmstoffhalter", "Url": "https://fiproductmedia.azureedge.net/media/Certification%20Documents/Certificates/fischer/ZD_EPD_INSULATION-FIXINGS_F_SEN_AIP_V1.pdf", "Language": "DE", "Filename": "ZD_EPD_INSULATION-FIXINGS_F_SEN_AIP_V1.pdf", "NameConvention": "ZD_EPD_F", "OutputType": "IP", "ValidFrom": "2022-02-22T00:00:00", "ValidTo": "2027-02-21T00:00:00", "Format": "pdf", "FileSize": 954561, "Type": "Environmental Product Declaration", "Position": 1, "TextList": [{"Key": "INFO1", "Value": "Umwelt Produktdeklaration f\u00fcr fischer D\u00e4mmstoffhalter"}], "AttributeList": [{"Name": "Zulassungsnummer", "Value": "EPD-FIW-20210314-CBD1-EN", "Key": "ZULANR"}], "CharacteristicValueNames": ["EPD - Europ\u00e4ische Umweltdeklaration"]}]}, "TextList": [{"Key": "BEZEICHNUNG", "Value": "DHK 45/80"}, {"Key": "ARTIKELLANG", "Value": "DHK 45/80"}, {"Key": "ECOMLANG", "Value": "fischer D\u00e4mmstoffhalter DHK 45 / 80"}, {"Key": "DOKBSTMARKCLAIM", "Value": "Der wirtschaftliche Kunststoff-D\u00e4mmstoffhalter f\u00fcr alle druckfesten D\u00e4mmplatten."}, {"Key": "DOKBSTMARKDESCR", "Value": "Der fischer D\u00e4mmstoffhalter DHK ist der wirtschaftliche Kunststoff-D\u00e4mmstoffhalter f\u00fcr D\u00e4mmplatten mit hoher Druckfestigkeit. Der DHK wird in der Durchsteckmontage mit dem Hammer eingeschlagen. Beim Einschlagen verspannen die Rippen im Bohrloch und der DHK wird optimal angepresst. Der fischer D\u00e4mmstoffhalter DHK ist geeignet, um D\u00e4mmplatten in Voll- und Lochstein, Beton und Porenbeton zu verankern."}, {"Key": "KAT_POS", "Value": "<div class=\"h9\">Der wirtschaftliche Kunststoff-D\u00e4mmstoffhalter f\u00fcr alle druckfesten D\u00e4mmplatten."}, {"Key": "KAT_ANWENDUNG_GEN", "Value": "<div class=\"h10\">Anwendungen</div><strong>Zur Fixierung von weichen und druckfesten D\u00e4mmstoffen in der hinterl\u00fcfteten Fassade wie:</strong><br><ul><li>Stein- / Glaswolle</li><li>PU-Hartschaumplatten</li><li>Holzwolleleichtbauplatten</li><li>Kork- / Kokosplatten</li><li>Polystyrol</li><li>Schaumglasplatten</li></ul>"}, {"Key": "KAT_BAUSTOFF_GEN", "Value": "<div class=\"h10\">Baustoffe</div><ul><li>Beton</li><li>Hohlblock aus Leichtbeton</li><li>Hochlochziegel</li><li>Kalksand-Lochstein</li><li>Kalksand-Vollstein</li><li>Naturstein mit dichtem Gef\u00fcge</li><li>Porenbeton</li><li>Vollstein aus Leichtbeton</li><li>Vollziegel</li></ul>"}, {"Key": "POCTOPFEATURESGEN", "Value": "<div class=\"h10\">Top Features</div><ul><li>Reduzierter Bohraufwand: Optimierte Geometrie reduziert Verankerungstiefe.</li><li>Einfache Schlagmontage spart Zeit und Kosten.</li></ul>"}, {"Key": "KAT_VORTEILNUTZEN", "Value": "<div class=\"h10\">Vorteile</div><ul><li>Die optimierte Geometrie des Spreizbereichs sorgt f\u00fcr eine geringe Verankerungstiefe und reduziert den Bohraufwand.</li><li>Flexible Stege im Tellerbereich passen sich dem D\u00e4mmstoff an und sorgen f\u00fcr dauerhaften Anpressdruck.</li><li>Die einfache Schlagmontage erm\u00f6glicht einen schnellen Setzvorgang und reduziert so den Arbeitsaufwand.</li><li>Der DHK 45 ist f\u00fcr den Einsatz in druckfesten D\u00e4mmplatten und Laibungen geeignet.</li></ul>"}, {"Key": "KAT_FUNKTION", "Value": "<div class=\"h10\">Funktionsweise / Montage</div><ul><li>Der DHK wird in Durchsteckmontage mit dem Hammer eingeschlagen.</li><li>Durch das Verspannen der Rippen im Bohrloch erh\u00e4lt der DHK den optimalen Anpressdruck.</li><li>Temperaturbereich im montierten Zustand: -40\u00b0C bis +80\u00b0C.</li></ul>"}, {"Key": "DOKBSTTOPFEATURE", "Value": "Reduzierter Bohraufwand: Optimierte Geometrie reduziert Verankerungstiefe."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Einfache Schlagmontage spart Zeit und Kosten."}], "Attributes": [{"AttributeKey": "BOHRERNENNDM", "AttributeName": "Bohrernenndurchmesser", "Unit": "[mm]", "Value": "8", "TechnicalName": "<div class=\"h6\">d<sub>0</sub></div>"}, {"AttributeKey": "MIN_BOHRLT", "AttributeName": "Min. Bohrlochtiefe", "Unit": "[mm]", "Value": "30", "TechnicalName": "<div class=\"h6\">h<sub>1</sub></div>"}, {"AttributeKey": "MIN_VERAN", "AttributeName": "Min. Verankerungstiefe", "Unit": "[mm]", "Value": "20", "TechnicalName": "<div class=\"h6\">h<sub>ef</sub></div>"}, {"AttributeKey": "DUEBELL", "AttributeName": "D\u00fcbell\u00e4nge", "Unit": "[mm]", "Value": "105", "TechnicalName": "<div class=\"h6\">l</div>"}, {"AttributeKey": "MAX_NUTZLG", "AttributeName": "Max. Dicke des Anbauteils", "Unit": "[mm]", "Value": "80", "TechnicalName": "<div class=\"h6\">t <sub>fix</sub></div>"}, {"AttributeKey": "FA_VERPACKVAR", "AttributeName": "Verpackungsvariante", "Value": "Faltschachtel"}, {"AttributeKey": "MENGE", "AttributeName": "Menge", "Unit": "[St\u00fcck]", "Value": "250"}, {"AttributeKey": "EAN_CODE", "AttributeName": "GTIN (EAN-Code)", "Value": "4006209808948"}], "FilterAttributes": [{"FilterId": "Montageart_A", "FilterName": "Montageart", "ValueList": ["Schlagd\u00fcbel"], "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "A", "ValueList": ["A"]}]}, {"FilterId": "Min. Verankerungstiefe_A", "FilterName": "Min. Verankerungstiefe", "ValueList": ["20"], "Unit": "[mm]", "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "A", "ValueList": ["A"]}]}, {"FilterId": "Baustoff_A", "FilterName": "Baustoff", "ValueList": ["Beton", "Hochlochziegel", "Kalksandlochstein KSL", "Kalksandvoll- und blockstein KS", "MZ3", "POR3"], "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "A", "ValueList": ["A"]}]}, {"FilterId": "Bohrernenndurchmesser_N", "FilterName": "Bohrernenn&shy;durchmesser", "Unit": "[mm]", "Value": "8", "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "N", "ValueList": ["N"]}]}]}