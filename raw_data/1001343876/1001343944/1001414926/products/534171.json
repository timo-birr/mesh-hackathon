, {"DocumentId": "df1db3a09f433c30c388ca5f04c80fba", "Name": "Verkaufsunterlagen", "Description": "fischer Putzd\u00fcbel FIF", "Url": "https://fiproductmedia.azureedge.net/media/Marketing%20Materials/Sales%20Materials/VKU_FIF_F_SDE_AIP_V1.pdf", "Language": "DE", "Filename": "VKU_FIF_F_SDE_AIP_V1.pdf", "NameConvention": "VKU_F", "OutputType": "IP", "Format": "pdf", "FileSize": 2354340, "Type": "Sales Materials", "Position": 1, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Verkaufsunterlagen"}, {"Key": "INFO1", "Value": "fischer Putzd\u00fcbel FIF"}, {"Key": "KAT_NEU_PRODBILD", "Value": "Verkaufsunterlagen"}], "CharacteristicValueNames": ["Verkaufsunterlagen"]}], "Approvals": [{"DocumentId": "e9eff48d3e466b343e46e3c55c608225", "Name": "ETA - Europ\u00e4ische Technische Bewertung", "Url": "https://fiproductmedia.azureedge.net/media/Certification%20Documents/ETA%20European%20Technical%20Assessment/fischer/ZD_ETAB_F_15-0006_SEN_AIP_V2.pdf", "Language": "DE", "Filename": "ZD_ETAB_F_15-0006_SEN_AIP_V2.pdf", "NameConvention": "ZD_ETAB_F", "OutputType": "IP", "ValidFrom": "2018-05-31T00:00:00", "Format": "pdf", "FileSize": 1140891, "Type": "ETA - European Technical Assessment", "Position": 1, "AttributeList": [{"Name": "Zulassungsnummer", "Value": "ETA-15/0006", "Key": "ZULANR"}], "CharacteristicValueNames": ["ETA - Europ\u00e4ische Technische Bewertung"], "ChildDocumentIds": ["baa24bc557c5bc5d4e67c6880e93f446"]}, {"DocumentId": "baa24bc557c5bc5d4e67c6880e93f446", "Name": "DOP - Declaration of Performance", "Description": "Leistungserkl\u00e4rung f\u00fcr fischer FIF-CS-8 (Kunststoffd\u00fcbel f\u00fcr die Verwendung in Beton und Mauerwerk)", "Url": "https://fiproductmedia.azureedge.net/media/Certification%20Documents/DOP%20Documents/fischer/ZD_DOP_DE_FIF-CS-8_DOP-0254_ETA-15_0006.pdf", "Language": "DE", "Filename": "ZD_DOP_DE_FIF-CS-8_DOP-0254_ETA-15_0006.pdf", "NameConvention": "ZD_DOP_F", "OutputType": "IP", "ValidFrom": "2021-01-15T00:00:00", "Format": "pdf", "FileSize": 1194148, "Type": "DOP - Declaration of Performance", "Position": 3, "TextList": [{"Key": "INFO1", "Value": "Leistungserkl\u00e4rung f\u00fcr fischer FIF-CS-8 (Kunststoffd\u00fcbel f\u00fcr die Verwendung in Beton und Mauerwerk)"}], "AttributeList": [{"Name": "Zulassungsnummer", "Value": "DoP No. 0254", "Key": "ZULANR"}], "CharacteristicValueNames": ["DOP - Declaration of Performance"], "ParentDocumentId": "e9eff48d3e466b343e46e3c55c608225"}, {"DocumentId": "5776128fad38be5f2ed8d6793a23a747", "Name": "EPD - Europ\u00e4ische Umweltdeklaration", "Description": "Umwelt Produktdeklaration f\u00fcr fischer D\u00e4mmstoffhalter", "Url": "https://fiproductmedia.azureedge.net/media/Certification%20Documents/Certificates/fischer/ZD_EPD_INSULATION-FIXINGS_F_SEN_AIP_V1.pdf", "Language": "DE", "Filename": "ZD_EPD_INSULATION-FIXINGS_F_SEN_AIP_V1.pdf", "NameConvention": "ZD_EPD_F", "OutputType": "IP", "ValidFrom": "2022-02-22T00:00:00", "ValidTo": "2027-02-21T00:00:00", "Format": "pdf", "FileSize": 954561, "Type": "Environmental Product Declaration", "Position": 1, "TextList": [{"Key": "INFO1", "Value": "Umwelt Produktdeklaration f\u00fcr fischer D\u00e4mmstoffhalter"}], "AttributeList": [{"Name": "Zulassungsnummer", "Value": "EPD-FIW-20210314-CBD1-EN", "Key": "ZULANR"}], "CharacteristicValueNames": ["EPD - Europ\u00e4ische Umweltdeklaration"]}]}, "TextList": [{"Key": "BEZEICHNUNG", "Value": "FIF-CS 8/340"}, {"Key": "ARTIKELLANG", "Value": "FIF-CS 8/340"}, {"Key": "ECOMLANG", "Value": "fischer Putzd\u00fcbel FIF-CS 8/340"}, {"Key": "DOKBSTMARKCLAIM", "Value": "Der wirtschaftliche Schraubd\u00fcbel f\u00fcr alle WDVS D\u00e4mmstoffarten."}, {"Key": "DOKBSTMARKDESCR", "Value": "Der ETA zugelassene fischer Putzd\u00fcbel FIF-CS ist ideal, um druckfeste D\u00e4mmstoffe bis 340 mm Dicke f\u00fcr W\u00e4rmed\u00e4mmstoffsysteme wie Polystyrold\u00e4mmplatten oder Mineralwollplatten, in Beton, Voll- und Lochstein, haufwerksporigem Leichtbeton und Porenbeton sicher zu befestigen. Die einheitliche Setztiefe f\u00fcr alle Baustoffe vereinfacht die Montage. Der Schraubd\u00fcbel besteht aus einer grauen D\u00fcbelh\u00fclse mit aufgestecktem Oktagonalem Teller und einer Compound-Schraube. Die Compound-Schraube minimiert die W\u00e4rmebr\u00fccke. Dadurch werden D\u00fcbelabzeichnungen an der Fassade verhindert. Der FIF-CS 8 wird mit einem handels\u00fcblichen Schrauber gesetzt."}, {"Key": "KAT_POS", "Value": "<div class=\"h9\">Der wirtschaftliche Schraubd\u00fcbel f\u00fcr alle WDVS D\u00e4mmstoffarten."}, {"Key": "KAT_ANWENDUNG_GEN", "Value": "<div class=\"h10\">Anwendungen</div><ul><li>Befestigung von WDVS-D\u00e4mmplatten auf Beton und Mauerwerk</li><li>Oberfl\u00e4chenb\u00fcndige Montage in allen g\u00e4ngigen D\u00e4mmstoffarten</li></ul>"}, {"Key": "KAT_BAUSTOFF_GEN", "Value": "<div class=\"h10\">Baustoffe</div><ul><li>Baustoffklassen A, B, C, D, E</li><li>Beton</li><li>Beton (Wetterschale)</li><li>Mauerziegel</li><li>Kalksand-Vollstein</li><li>Hohlblock aus Leichtbeton</li><li>Hochlochziegel</li><li>Kalksand-Lochstein</li><li>Haufwerksporiger Leichtbeton</li><li>Porenbeton</li></ul>"}, {"Key": "POCTOPFEATURESGEN", "Value": "<div class=\"h10\">Top Features</div><ul><li>Hohe Lastaufnahme durch innovative Compoundschraube.</li><li>Geringe Setztiefe f\u00fcr alle Baustoffklassen reduziert den Bohraufwand. Das spart Zeit und Kosten.</li><li>Keine Abzeichnungen an der Putzoberfl\u00e4che: Glasfaserverst\u00e4rkte Compoundschraube mit Stahlspitze reduziert Transmissionsw\u00e4rme.</li></ul>"}, {"Key": "KAT_VORTEILNUTZEN", "Value": "<div class=\"h10\">Vorteile</div><ul><li>Die Compoundschraube (Stahl-Kunststoff Kombination) minimiert die W\u00e4rmebr\u00fccke. Dadurch entstehen keine D\u00fcbelabzeichnungen an der Fassade.</li><li>Minimale Einbindetiefe von 35 mm im tragenden Untergrund sorgt f\u00fcr einen geringen Bohrerverschlei\u00df und reduziert die Bohrzeit.</li><li>Bei der d\u00e4mmstoffb\u00fcndigen Montage sorgt der sehr d\u00fcnn auslaufende Teller f\u00fcr das optimale Anschmiegen an die D\u00e4mmplatte und den Auftrag d\u00fcnner Armierungsschichten.</li><li>F\u00fcr D\u00e4mmstoffdicken bis 340 mm.</li><li>Eine Einbindetiefe f\u00fcr alle Baustoffe.</li></ul>"}, {"Key": "KAT_FUNKTION", "Value": "<div class=\"h10\">Funktionsweise / Montage</div><ul><li>Der Befestiger wird durch die D\u00e4mmung in das Bohrloch gesteckt und anschlie\u00dfend verschraubt.</li><li>L\u00e4ngen 8/60 bis 8/180 mit handels\u00fcblichen TX30 Bit. L\u00e4ngen 8/200 bis 8/300 mit handels\u00fcblichen TX25 Bit > 180 mm.</li></ul>"}, {"Key": "DOKBSTTOPFEATURE", "Value": "Hohe Lastaufnahme durch innovative Compoundschraube."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Geringe Setztiefe f\u00fcr alle Baustoffklassen reduziert den Bohraufwand. Das spart Zeit und Kosten."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Keine Abzeichnungen an der Putzoberfl\u00e4che: Glasfaserverst\u00e4rkte Compoundschraube mit Stahlspitze reduziert Transmissionsw\u00e4rme."}], "Attributes": [{"AttributeKey": "ZULASSUNG_E", "AttributeName": "ETA-Zulassung", "Value": "relevant"}, {"AttributeKey": "BOHRERNENNDM", "AttributeName": "Bohrernenndurchmesser", "Unit": "[mm]", "Value": "8", "TechnicalName": "<div class=\"h6\">d<sub>0</sub></div>"}, {"AttributeKey": "MIN_BOHRLT", "AttributeName": "Min. Bohrlochtiefe", "Unit": "[mm]", "Value": "45", "TechnicalName": "<div class=\"h6\">h<sub>1</sub></div>"}, {"AttributeKey": "MIN_VERAN", "AttributeName": "Min. Verankerungstiefe", "Unit": "[mm]", "Value": "35", "TechnicalName": "<div class=\"h6\">h<sub>ef</sub></div>"}, {"AttributeKey": "DUEBELL", "AttributeName": "D\u00fcbell\u00e4nge", "Unit": "[mm]", "Value": "388", "TechnicalName": "<div class=\"h6\">l</div>"}, {"AttributeKey": "TELLER_DM", "AttributeName": "Teller-\u00d8", "Unit": "[mm]", "Value": "60"}, {"AttributeKey": "MENGE", "AttributeName": "Menge", "Unit": "[St\u00fcck]", "Value": "100"}, {"AttributeKey": "EAN_CODE", "AttributeName": "GTIN (EAN-Code)", "Value": "4048962231311"}], "FilterAttributes": [{"FilterId": "Teller-\u00d8_N", "FilterName": "Teller-\u00d8", "Unit": "[mm]", "Value": "60", "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "N", "ValueList": ["N"]}]}, {"FilterId": "Min. Verankerungstiefe_A", "FilterName": "Min. Verankerungstiefe", "ValueList": ["35"], "Unit": "[mm]", "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "A", "ValueList": ["A"]}]}, {"FilterId": "Bohrernenndurchmesser_N", "FilterName": "Bohrernenn&shy;durchmesser", "Unit": "[mm]", "Value": "8", "Attributes": [{"AttributeKey": "DATATYPE", "AttributeName": "Suchfeld Datentyp", "Value": "N", "ValueList": ["N"]}]}]}