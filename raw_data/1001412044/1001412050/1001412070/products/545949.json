, {"DocumentId": "53ccb4bbef443b1accf46535de461f4c", "Url": "https://media.fischer.group/v7/_pim-media-prod_/Mounting%20Pictures/Mounting%20Pictures%20fischer/W6_M_B_10_BADKLEBER_F_SALL_AQQ_V1.jpg?w=276&h=210&func=fit&bg_colour=white", "Filename": "W6_M_B_10_BADKLEBER_F_SALL_AQQ_V1.jpg", "NameConvention": "M_G_01_F", "OutputType": "PP", "Format": "jpg", "FileSize": 1357497, "Type": "Mounting Strip 1 Picture", "Position": 10, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Montagebild"}, {"Key": "KAT_NEU_PRODBILD", "Value": "Montagebild"}]}]}], "Documents": {"AdditionalDocuments": [{"DocumentId": "b275694b1faf42f1ab58f8bb56f7939e", "Name": "Sicherheitsdatenblatt", "Description": "Sicherheitsdatenblatt f\u00fcr Artikel 545949 Badkleber", "Url": "https://fiproductmedia.azureedge.net/media/Certification%20Documents/Safety%20Data%20Sheets/fischer/SDS_545949_DE-DE_BAD-KLEBER-2022-11-15.pdf", "Language": "DE", "Filename": "SDS_545949_DE-DE_BAD-KLEBER-2022-11-15.pdf", "NameConvention": "ZD_SDB_01_F", "OutputType": "IP", "Format": "pdf", "FileSize": 505549, "Type": "Safety Datasheet", "Position": 1, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Sicherheitsdatenblatt"}, {"Key": "INFO1", "Value": "Sicherheitsdatenblatt f\u00fcr Artikel 545949 Badkleber"}, {"Key": "KAT_NEU_PRODBILD", "Value": "Sicherheitsdatenblatt"}], "CharacteristicValueNames": ["Sicherheitsdatenblatt"]}, {"DocumentId": "f651f9b5c2e64b556e463a6de1bb3c69", "Name": "Verkaufsunterlagen", "Description": "BAD KLEBER", "Url": "https://fiproductmedia.azureedge.net/media/Marketing%20Materials/Sales%20Materials/VKU_BADKLEBER_F_SDE_AIP_V1.pdf", "Language": "DE", "Filename": "VKU_BADKLEBER_F_SDE_AIP_V1.pdf", "NameConvention": "VKU_F", "OutputType": "IP", "Format": "pdf", "FileSize": 158368, "Type": "Sales Materials", "Position": 1, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Verkaufsunterlagen"}, {"Key": "INFO1", "Value": "BAD KLEBER"}, {"Key": "KAT_NEU_PRODBILD", "Value": "Verkaufsunterlagen"}], "CharacteristicValueNames": ["Verkaufsunterlagen"]}, {"DocumentId": "d6d02e366e808d17fd63eb6d30490416", "Name": "Technisches Datenblatt", "Description": "BADKLEBER", "Url": "https://fiproductmedia.azureedge.net/media/Certification%20Documents/Technical%20Datasheets/fischer/TDB_01_BADKLEBER_F_SDE_AIP_V1.pdf", "Language": "DE", "Filename": "TDB_01_BADKLEBER_F_SDE_AIP_V1.pdf", "NameConvention": "TDB_01_F", "OutputType": "IP", "Format": "pdf", "FileSize": 175718, "Type": "Technical Datasheet", "Position": 1, "TextList": [{"Key": "BILDLEG_NEXMART", "Value": "Technisches Datenblatt"}, {"Key": "INFO1", "Value": "BADKLEBER"}, {"Key": "KAT_NEU_PRODBILD", "Value": "Technisches Datenblatt"}], "CharacteristicValueNames": ["Technisches Datenblatt"]}]}, "TextList": [{"Key": "BEZEICHNUNG", "Value": "Badkleber"}, {"Key": "ARTIKELLANG", "Value": "Badkleber"}, {"Key": "ECOMLANG", "Value": "fischer Badkleber"}, {"Key": "DOKBSTMARKCLAIM", "Value": "Badaccessoires befestigen, ohne zu bohren"}, {"Key": "DOKBSTMARKDESCR", "Value": "In Fliesen bohren ist nicht immer erlaubt. Ankleben ist da schon viel besser. Also, egal ob Handtuchhalter, Duschregal, Gardinenstangen, Schminkspiegel, Haken oder Toilettenpapierhalter: In Bad und K\u00fcche rettet der fischer Badkleber jede Fliese vor Bohrl\u00f6chern. Mit einer sehr schnellen Anfangshaftung und einer vollen Belastbarkeit nach zwei Stunden, ist der Badkleber schnell einsatzbereit. Der Kleber ist durch seine gute Best\u00e4ndigkeit und universelle Anwendung f\u00fcr beinahe jede Adapterplatte anwendbar. Der Badkleber erm\u00f6glicht das Anbringen von vielen Accessoires GANZ OHNE WERKZEUG."}, {"Key": "KAT_POS", "Value": "<div class=\"h9\">Badaccessoires befestigen, ohne zu bohren"}, {"Key": "KAT_ANWENDUNG_GEN", "Value": "<div class=\"h10\">Anwendungen</div>Zur einfachen Befestigung von Badaccessoires auf:<ul><li>allen Oberfl\u00e4chen</li></ul>"}, {"Key": "POCTOPFEATURESGEN", "Value": "<div class=\"h10\">Top Features</div><ul><li>Einsatzbereit durch eine schnelle Anfangshaftung und eine hohe Belastbarkeit in maximal 2 Stunden.</li><li>Universell und flexibel f\u00fcr jede Adapterplatte anwendbar.</li><li>Von glatten und festen Untergr\u00fcnden r\u00fcckstandsfrei entfernbar. Sehr geeignet f\u00fcr Oberfl\u00e4chen wie Fliesen und Badfliesen.</li><li>Es wird kein Werkzeug f\u00fcr die Befestigung ben\u00f6tigt. Bohrl\u00f6cher werden vermieden durch reine Klebekraft.</li><li>Von glatten und festen Untergr\u00fcnden r\u00fcckstandsfrei entfernbar. Sehr geeignet f\u00fcr Oberfl\u00e4chen wie Fliesen.</li></ul>"}, {"Key": "KAT_VORTEILNUTZEN", "Value": "<div class=\"h10\">Vorteile</div><ul><li>wasserbest\u00e4ndig</li><li>flexibel</li><li>absorbiert Vibrationen</li><li>transparent</li></ul>"}, {"Key": "KAT_FUNKTION", "Value": "<div class=\"h10\">Funktionsweise / Montage</div><ul><li>Mit dem Reinigungstuch Fliese und Adapterplatte reinigen.</li><li>Klebebeutel von unten bis zur Mitte des unteren Beutels aufrollen.</li><li>Mit zwei Daumen auf den Wulst dr\u00fccken, um die Kammernaht durchzudr\u00fccken!</li><li>Weiter bis zur Darstellung der Rolle aufrollen.</li><li>Die zwei Komponenten im aufgerollten Beutel 1 Minute durchkneten.</li><li>Beutel innerhalb 1 Minute \u00f6ffnen und Inhalt auf Adapterplatte aufbringen.</li><li>Adapterplatte f\u00fcr 10 Sekunden fest andr\u00fccken.</li><li>Nach 15 Minuten leicht, nach 2 Stunden voll belastbar.</li><li>Vor Demontage Klebstoff mit einem F\u00f6hn oder hei\u00dfem Wasser erw\u00e4rmen.</li><li>Zum Entfernen mit Zange abl\u00f6sen.</li><li>Kleber\u00fcckst\u00e4nde mit Spachtel l\u00f6sen.</li></ul>"}, {"Key": "DOKBSTTOPFEATURE", "Value": "Einsatzbereit durch eine schnelle Anfangshaftung und eine hohe Belastbarkeit in maximal 2 Stunden."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Universell und flexibel f\u00fcr jede Adapterplatte anwendbar."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Von glatten und festen Untergr\u00fcnden r\u00fcckstandsfrei entfernbar. Sehr geeignet f\u00fcr Oberfl\u00e4chen wie Fliesen und Badfliesen."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Es wird kein Werkzeug f\u00fcr die Befestigung ben\u00f6tigt. Bohrl\u00f6cher werden vermieden durch reine Klebekraft."}, {"Key": "DOKBSTTOPFEATURE", "Value": "Von glatten und festen Untergr\u00fcnden r\u00fcckstandsfrei entfernbar. Sehr geeignet f\u00fcr Oberfl\u00e4chen wie Fliesen."}], "Attributes": [{"AttributeKey": "INHALT", "AttributeName": "Inhalt", "Value": "2 x Klebebeutel 6 g, 2 x Reinigungst\u00fccher"}, {"AttributeKey": "MENGE", "AttributeName": "Menge", "Unit": "[St\u00fcck]", "Value": "1"}, {"AttributeKey": "EAN_CODE", "AttributeName": "GTIN (EAN-Code)", "Value": "4048962325430"}]}